/project-frontend.DS_Store
/project-frontend*.pem

# debug
/project-frontendnpm-debug.log*
/project-frontendyarn-debug.log*
/project-frontendyarn-error.log*
/project-frontend.pnpm-debug.log*

# local env files
/project-frontend.env*.local

# vercel
/project-frontend.vercel

# typescript
/project-frontend*.tsbuildinfo
/project-frontendnext-env.d.ts

